   "source": [
    "def find_squared_root ( a ):\n",
    "    EPSILON = 0.001\n",
    "    x = a\n",
    "    while True:\n",
    "        x_n1 = x - (x*x-a)/(2*x)\n",
    "        if abs(x_n1-x) < EPSILON:\n",
    "            return x_n1\n",
    "        x = x_n1"
   ]
  },
